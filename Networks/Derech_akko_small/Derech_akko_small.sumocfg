<configuration>
    <input>
       <net-file value="Derech_akko_small.net.xml"/>
       <route-files value="flows/generated/Derech_akko_small.rou.xml"/>
    </input>
	<time-to-teleport value="-1"/>
	<time>
        <begin value="0"/>
        <end value="86400"/>
        <step-length value="1"/>
    </time>
    <gui_only>
        <gui-settings-file value="../gui_settings.xml"/>
        <game value="false"/>
        <start value="false"/>
    </gui_only>
    <output>
        <summary-output value="Derech_akko_small.out.xml"/>
    </output>
    <processing>
	<ignore-route-errors value="true"/>
    </processing>

    <report>
        <verbose value="true"/>
        <duration-log.statistics value="true"/>
        <no-step-log value="true"/>
    </report>

</configuration>